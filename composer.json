{
    "name": "codica-studio/adminbackup",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/adminbackup",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "laravel/nova": "^3.0",
        "nesbot/carbon": "^2.31",
        "spatie/laravel-backup": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\BackupTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\BackupTool\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\BackupTool\\BackupToolServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
